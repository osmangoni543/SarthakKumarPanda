<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test name="Test">
    <classes>
    
      <class name="com.automation.TentNG.day7.Login">
      <methods>
      
      <include name = "verifyValidLogin"/>
      <include name = "verifyInvalidLogin"/>
      <exclude name = "verifyNoLogin"/>
      
      </methods>
      </class>
      
      <class name="com.automation.TentNG.day7.Product">
      <methods>
      
      <include name = "verifyValidProduct"/>
      <include name = "verifyInvalidProduct"/>
      <exclude name = "verifyNoProduct"/>
      
      </methods>
      </class>
      
      <class name="com.automation.TentNG.day7.Register">
      <methods>
      
      <include name = "verifyValidRegister"/>
      <include name = "verifyInvalidRegister"/>
      <exclude name = "verifyNoRegister"/>
      
      </methods>
      </class>
      
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
